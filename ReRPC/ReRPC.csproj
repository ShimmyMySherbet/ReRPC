<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <PackageId>ShimmyMySherbet.ReRPC</PackageId>
    <Title>ReRPC</Title>
    <Authors>ShimmyMySherbet</Authors>
    <Description>Yet another lightweight and versatile RPC Socket. Designed to be easy to use, and with KISS principles in mind.</Description>
    <Copyright>ShimmyMySherbet</Copyright>
    <PackageProjectUrl>https://github.com/ShimmyMySherbet/ReRPC</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ShimmyMySherbet/ReRPC</RepositoryUrl>
    <PackageTags>RPC</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
